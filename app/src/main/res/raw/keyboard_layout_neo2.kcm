  rshift+lshift: '\u27e8'
  ralt+rshift: '\u221e'
}

key 9 {
  label: '\u00e7'
  base: '\u00e7'
  lshift: '9'
  capslock: '\u00c7'
  capslock+lshift: '\u00e7'
  ralt, scrolllock: '^'
  scrolllock+ralt: '9'
  ralt+lshift, ralt+capslock: '('
  scrolllock+ralt+lshift, scrolllock+ralt+capslock: '\u201c'
  scrolllock+ralt+capslock+lshift: '9'
  rshift: '\u2018'
  rshift+lshift: '\u27e9'
  ralt+rshift: '\u221d'
}

key 0 {
  label: '\u00e0'
