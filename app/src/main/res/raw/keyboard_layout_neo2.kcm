key EQUALS {
  label: '='
  base: '='
  lshift, capslock: '+'
  capslock+lshift: '='
  ralt, scrolllock: '}' # \u0307 c'est dead point en haut.
  ralt+lshift, ralt+capslock: '\u00a7' # ceci ne marche pas: fallback FORWARD_DEL
  scrolllock+ralt: '\u0302'
  scrolllock+ralt+lshift, scrolllock+ralt+capslock: '\u030c'
  scrolllock+ralt+capslock+lshift: '\u0302'
  rshift: none
  # todo rshift => rotate? dead turning?
  rshift+lshift: '\u0309'
  ralt+rshift: '\u0323'
}

key 1 {
  label: '&'
  base: '&'
  lshift, capslock: '1'
  capslock+lshift: '&'
  ralt, scrolllock: '\u007c'
  ralt+lshift, ralt+capslock: '!'
  scrolllock+ralt: '4'
  scrolllock+ralt+capslock+lshift: '6'
  rshift: '7'
  rshift+lshift: '8'
  ralt+rshift: '9'
}

key 2 {
  label: '\u00e9'
  base: '\u00e9'
  lshift: '2'
  capslock: '\u00c9'
  capslock+lshift: '\u00e9'
  ralt, scrolllock: '~'
  ralt+lshift, ralt+capslock: '>'
  scrolllock+ralt: '2'
  scrolllock+ralt+lshift, scrolllock+ralt+capslock: '\u00a7'
  scrolllock+ralt+capslock+lshift: '2'
  rshift: '\u00b2'
  rshift+lshift: '\u2082'
  ralt+rshift: '\u2228'
}

key 3 {
  label: '"'
  base: '"'
  lshift, capslock: '3'
  capslock+lshift: '"'
  ralt, scrolllock: '#'
  ralt+lshift, ralt+capslock: '<'
  scrolllock+ralt: '3'
  scrolllock+ralt+lshift, scrolllock+ralt+capslock: '<'
  scrolllock+ralt+capslock+lshift: '3'
  rshift: '\u00b3'
  rshift+lshift: '\u2083'
  ralt+rshift: '\u2227'
}

key 4 {
  label: '\u0027' # '
  base: '\u0027'
  lshift, capslock: '4'
  capslock+lshift: '\u0027'
  ralt, scrolllock: '{' # tiens none fait que ca ouvre un menu et le referme tout de suite.
  ralt+lshift, ralt+capslock: '$'
  scrolllock+ralt: '4'
  scrolllock+ralt+lshift, scrolllock+ralt+capslock: '\u00bb'
  scrolllock+ralt+capslock+lshift: '4'
  rshift: '\u203a'
  rshift+lshift: '\u2640'
  ralt+rshift: '\u22a5'
}

key 5 {
  label: '('
  base: '('
  lshift, capslock: '5'
  capslock+lshift: '('
  ralt, scrolllock: '['
  ralt+lshift, ralt+capslock: '%'
  scrolllock+ralt: '5'
  scrolllock+ralt+lshift, scrolllock+ralt+capslock: '\u00ab'
  scrolllock+ralt+capslock+lshift: '5'
  rshift: '\u2039'
  rshift+lshift: '\u2642'
  ralt+rshift: '\u2221'
}

key 6 {
  label: '-'
  base: '-'
  lshift, capslock: '6'
  capslock+lshift: '-'
  ralt, scrolllock: '|'
  ralt+lshift, ralt+capslock: '^'
  scrolllock+ralt: '6'
  scrolllock+ralt+lshift, scrolllock+ralt+capslock: '\u0024'
  scrolllock+ralt+capslock+lshift: '6'
  rshift: '\u00a2'
  rshift+lshift: '\u26a5'
  ralt+rshift: '\u2225'
}

key 7 {
  label: '\u00e8'
  base: '\u00e8'
  lshift: '7'
  capslock: '\u00c8'
  capslock+lshift: '\u00e8'
  ralt, scrolllock: '`'
  ralt+lshift, ralt+capslock: '&'
  scrolllock+ralt: '7'
  scrolllock+ralt+lshift, scrolllock+ralt+capslock: '\u20ac'
  scrolllock+ralt+capslock+lshift: '7'
  rshift: '\u00a5'
  rshift+lshift: '\u03f0'
  ralt+rshift: '\u2192'
}

key 8 {
  label: '_'
  base: '_'
  lshift, capslock: '8'
  capslock+lshift: '_'
