  scrolllock+ralt: 'l'
  scrolllock+ralt+lshift, scrolllock+ralt+capslock: 'L'
  scrolllock+ralt+capslock+lshift: 'l'
  rshift: '['
  rshift+lshift: '\u03bb'
  ralt+rshift: '\u039b'
}

key C {
